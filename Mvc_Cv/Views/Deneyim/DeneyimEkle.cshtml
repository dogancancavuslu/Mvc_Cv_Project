
@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>   Yeni Deneyim Ekleme</h4>
<br />
<form method="post">
    <input type="text" name="Baslik" class="form-control" placeholder="Firma Adı Giriniz" />
    <br />
    <input type="text" name="AltBaslik" class="form-control" placeholder="Pozisyonunuzu Giriniz" />
    <br />
    <textarea placeholder= "Açıklama Giriniz" name="Aciklama" class="form-control" rows="5"> </textarea>
    <br />
    <input type="text" name="Tarih" class="form-control" placeholder="Tarih Giriniz (Gün.Ay.Yıl)" />
    <br />  
    <button class="btn btn-info">  KAYDET  </button>

</form>


    
    